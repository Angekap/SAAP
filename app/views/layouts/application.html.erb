<!-- end: Favicon and Touch Icons -->

</head>

<body>
  <%= render 'layouts/navbar' %>
<div class="container">
  <div class="row">
    <%= render 'layouts/sidebar' %>

    <!-- start: Content -->
    <div id="content" class="col-lg-10 col-sm-11" style='min-height: 400px'>


      <div class="row">
        <%= yield %>

      </div>

    </div>
    <!-- end: Content -->

  </div><!--/row-->

</div><!--/container-->

<div class="clearfix"></div>

<footer>
  <p>
  <span style="text-align:left;float:left">&copy; 2013 </span>
