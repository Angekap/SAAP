<div class='row'>
  <div class='col-lg-12'>
    <div class='box'>
      <div class='box-header'>
        <h2><i class="icon-list"></i>
          <%= I18n.t('headers.list_resource', resource: Professor.model_name.human)%>
        </h2>
      </div>
      <div class='box-content'>
        <table class='table table-bordere table-condensed table-striped'>
          <thead>
            <tr>
              <th><%= Professor.human_attribute_name(:username) %></th>
              <th><%= Professor.human_attribute_name(:name) %></th>
              <th><%= Professor.human_attribute_name(:email) %></th>
              <th style='width: 10%'><%= I18n.t('headers.options') %></th>
            </tr>
          </thead>
          <tbody>
            <%- @professors.each do |professor|  %>
              <tr>
                <td><%= professor.username %></td>
                <td><%= professor.name %></td>
                <td><%= professor.email %></td>
                <td></td>
              </tr>
            <%- end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
