<%- @crowds_with_groups.each_pair do |crowd, groups| %>
  <div class='col-lg-6'>
    <div class='row'>
      <div class='col-lg-12 col-md-12 col-sm-12'>
        <div class='box'>
          <div class='box-header'>
            <h2>
              <%= crowd %>
            </h2>
          </div>
          <div class='box-content'>
            <ul class='dashboard-list'>
              <%- groups.each do |group| %>
                <li>
                  <%= link_to group.enunciation, group_path(group) %>
                </li>
              <%- end %>
            </ul>
          </div>

        </div>
      </div>
    </div>
  </div>
<%- end %>


